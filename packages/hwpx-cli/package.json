{
  "name": "@ubermensch1218/hwpx-cli",
  "version": "0.1.0",
  "description": "CLI tool for HWPX file operations",
  "type": "module",
  "bin": {
    "hwpxtool": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "hwpx",
    "hwp",
    "cli",
    "korean",
    "document"
  ],
  "author": "ubermensch1218",
  "license": "MIT",
  "dependencies": {
    "@ubermensch1218/hwpx-tools": "workspace:*",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
